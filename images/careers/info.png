<svg xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 400 400">
  <!-- خلفية الصورة -->
  <rect width="400" height="400" fill="#3498db" />
  
  <!-- رمز الكمبيوتر -->
  <rect x="100" y="120" width="200" height="150" rx="10" fill="#2c3e50" />
  <rect x="120" y="140" width="160" height="100" rx="5" fill="#ecf0f1" />
  
  <!-- رمز الشاشة -->
  <rect x="140" y="160" width="120" height="60" fill="#3498db" />
  <rect x="150" y="170" width="100" height="10" fill="#ffffff" />
  <rect x="150" y="190" width="80" height="10" fill="#ffffff" />
  <rect x="150" y="210" width="60" height="10" fill="#ffffff" />
  
  <!-- قاعدة الكمبيوتر -->
