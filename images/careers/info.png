  <circle cx="80" cy="80" r="30" fill="#e74c3c" />
  <circle cx="80" cy="80" r="15" fill="#c0392b" />
  
  <!-- رمز الكود -->
  <rect x="40" y="300" width="60" height="60" rx="5" fill="#27ae60" />
  <rect x="50" y="310" width="40" height="5" fill="#ffffff" />
  <rect x="50" y="320" width="30" height="5" fill="#ffffff" />
  <rect x="50" y="330" width="35" height="5" fill="#ffffff" />
  <rect x="50" y="340" width="25" height="5" fill="#ffffff" />
  
  <!-- رمز السحابة -->
  <circle cx="320" cy="320" r="25" fill="#95a5a6" />
  <circle cx="340" cy="310" r="20" fill="#95a5a6" />
  <circle cx="360" cy="320" r="25" fill="#95a5a6" />
  <rect x="320" y="320" width="40" height="25" fill="#95a5a6" />
